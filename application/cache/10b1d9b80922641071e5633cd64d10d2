	$(function () {
		// var body_width = 1000, logo_width = 220;
		// navbar_width = $('#navbar-nav').width();
		// var $nav_li = $('#navbar-nav>li');
		// padding_width = Math.floor((body_width - logo_width - navbar_width)/($nav_li.length*2));
