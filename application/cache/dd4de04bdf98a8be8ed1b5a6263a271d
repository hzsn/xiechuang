<script type="text/javascript">
	$(function(){
		
		var height = $(document).height() - $('body').height();
		//让footer固定在页面的底部，
		//如果body的height小于document的height的话,在footer前加一个适当高度的div，把页面撑开
		if (height > 0) {
			$('footer').before('<div style="height:'+height+'px;"></div>');
		}
	});
